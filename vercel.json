{
  "version": 2,
  "buildCommand": "echo 'Static site - no build needed'",
  "outputDirectory": ".",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/:path*"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/services",
      "destination": "/services.html"
    },
    {
      "source": "/news",
      "destination": "/news.html"
    },
    {
      "source": "/products",
      "destination": "/products.html"
    },
    {
      "source": "/pricing",
      "destination": "/pricing.html"
    },
    {
      "source": "/faq",
      "destination": "/faq.html"
    },
    {
      "source": "/contact",
      "destination": "/contact.html"
    },
    {
      "source": "/quote",
      "destination": "/quote.html"
    },
    {
      "source": "/toronto",
      "destination": "/toronto.html"
    },
    {
      "source": "/mississauga",
      "destination": "/mississauga.html"
    },
    {
      "source": "/vaughan",
      "destination": "/vaughan.html"
    },
    {
      "source": "/oakville",
      "destination": "/oakville.html"
    },
    {
      "source": "/richmond-hill",
      "destination": "/richmond-hill.html"
    }
  ]
}
